# To sort a file:
sort <file>

# To sort a file by keeping only unique:
sort -u <file>

# To sort a file and reverse the result:
sort -r <file>

# To sort a file randomly:
sort -R <file>

# c is a union b
sort a b | uniq > c

# c is a intersect b
sort a b | uniq -d > c

# c is set difference a - b
sort a b b | uniq -u > c
