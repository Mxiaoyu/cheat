# To sort a file:
sort <file>

# To sort a file by keeping only unique:
sort -u <file>

# To sort a file and reverse the result:
sort -r <file>

# To sort a file randomly:
sort -R <file>

# c is a union b
sort a b | uniq > c

# c is a intersect b
sort a b | uniq -d > c

# c is set difference a - b
sort a b b | uniq -u > c

# 按照某一列排序
sort -k n <file>

# 指定分隔符
sort -k 1 -t , <file>
